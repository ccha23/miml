    "# Define the function for ploting\n",
    "def show_image(x, idx, title=None):\n",
    "    x = x.view(batch_size, 28, 28)\n",
    "\n",
    "    fig = plt.figure()\n",
    "    if title is not None:\n",
    "        plt.suptitle(title)\n",
    "    plt.imshow(x[idx].cpu().numpy())"
   ]
